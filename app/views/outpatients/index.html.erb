<p id="notice"><%= notice %></p>

<h1>Outpatients</h1>

<table>
  <thead>
    <tr>
      <th>Dt</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outpatients.each do |outpatient| %>
      <tr>
        <td><%= outpatient.dt %></td>
        <td><%= link_to 'Show', outpatient %></td>
        <td><%= link_to 'Edit', edit_outpatient_path(outpatient) %></td>
        <td><%= link_to 'Destroy', outpatient, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Outpatient', new_outpatient_path %>
