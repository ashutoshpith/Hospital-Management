<p id="notice"><%= notice %></p>

<h1>Doctortables</h1>

<table>
  <thead>
    <tr>
      <th>Doctor name</th>
      <th>Doctor sp</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @doctortables.each do |doctortable| %>
      <tr>
        <td><%= doctortable.doctor_name %></td>
        <td><%= doctortable.doctor_sp %></td>
        <td><%= link_to 'Show', doctortable %></td>
        <td><%= link_to 'Edit', edit_doctortable_path(doctortable) %></td>
        <td><%= link_to 'Destroy', doctortable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Doctortable', new_doctortable_path %>
