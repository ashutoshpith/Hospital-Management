
<%= simple_form_for(@doctortable) do |f| %>
  <%= f.error_notification %>
  <%= f.error_notification message: f.object.errors[:base].to_sentence if f.object.errors[:base].present? %>

  <div class="form-inputs">
    <%= f.collection_select :outpatient_id, Outpatient.order(:dt), :id, :dt, prompt: "Choose a Date" %>

    <%= f.input :doctor_name %>
    <%= f.input :doctor_sp %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
